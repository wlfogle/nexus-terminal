# Nexus Terminal Mobile Configuration

# Backend API Configuration
API_BASE_URL=https://api.nexusterminal.com
WEBSOCKET_URL=wss://ws.nexusterminal.com
API_VERSION=v1
API_TIMEOUT=30000

# Security Configuration
ENCRYPTION_KEY=your-32-character-encryption-key-here
JWT_SECRET=your-jwt-secret-key-here
SESSION_TIMEOUT=3600000

# Feature Flags
FEATURE_AI_ENABLED=true
FEATURE_ENTERPRISE_ENABLED=true
FEATURE_ANALYTICS_ENABLED=true
FEATURE_CRASH_REPORTING_ENABLED=true
FEATURE_BIOMETRIC_AUTH_ENABLED=true
FEATURE_CLOUD_SYNC_ENABLED=true
FEATURE_ADVANCED_MONITORING_ENABLED=true
FEATURE_AUTOMATION_ENABLED=true

# AI/ML Configuration
TENSORFLOW_BACKEND=rn-platform
AI_MODEL_ENDPOINT=https://ai.nexusterminal.com
NEURAL_NETWORK_CACHE_SIZE=50

# Monitoring & Analytics
ANALYTICS_ENDPOINT=https://analytics.nexusterminal.com
CRASH_REPORTING_ENDPOINT=https://crashes.nexusterminal.com
PERFORMANCE_MONITORING_ENDPOINT=https://performance.nexusterminal.com
ANALYTICS_BATCH_SIZE=100
ANALYTICS_FLUSH_INTERVAL=30000

# Enterprise Configuration
LDAP_SERVER_URL=ldap://directory.company.com:389
SSO_PROVIDER_URL=https://sso.company.com
ENTERPRISE_API_ENDPOINT=https://enterprise.nexusterminal.com
COMPLIANCE_ENDPOINT=https://compliance.nexusterminal.com

# File Transfer Configuration
MAX_FILE_UPLOAD_SIZE=104857600
CHUNK_SIZE=1048576
CONCURRENT_UPLOADS=3
TEMP_DIRECTORY=/tmp/nexus-terminal

# Terminal Configuration
DEFAULT_TERMINAL_THEME=monokai
DEFAULT_FONT_SIZE=14
DEFAULT_FONT_FAMILY=courier-new
TERMINAL_SCROLLBACK_SIZE=10000
COMMAND_HISTORY_SIZE=1000

# Security Settings
PASSWORD_MIN_LENGTH=8
PASSWORD_REQUIRE_SPECIAL=true
PASSWORD_REQUIRE_NUMBERS=true
PASSWORD_REQUIRE_UPPERCASE=true
AUTO_LOCK_TIMEOUT=300000
MAX_LOGIN_ATTEMPTS=5
LOCKOUT_DURATION=900000

# Cloud Sync Configuration
CLOUD_SYNC_PROVIDER=aws-s3
CLOUD_SYNC_BUCKET=nexus-terminal-sync
CLOUD_SYNC_REGION=us-east-1
SYNC_INTERVAL=300000
CONFLICT_RESOLUTION=latest-wins

# Development Configuration
DEBUG_ENABLED=false
LOG_LEVEL=info
DEV_TOOLS_ENABLED=false
MOCK_DATA_ENABLED=false
TEST_MODE_ENABLED=false

# Production Configuration
ENVIRONMENT=production
BUILD_NUMBER=1
VERSION_NUMBER=1.0.0
APP_STORE_URL=https://apps.apple.com/app/nexus-terminal
GOOGLE_PLAY_URL=https://play.google.com/store/apps/details?id=com.nexusterminal
